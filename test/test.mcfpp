namespace test;

func main(){
    solveHanoi(3, 1, 3, 2);
}

func solveHanoi(int n, int f, int to, int aux) {
    if(n == 1) {
        print("Move disk 1 from rod " + f.toText() + " to rod " + to.toText());
        return;
    }
    solveHanoi(n-1, f, aux, to);
    print("Move disk " + n.toText() + " from rod " + f.toText() + " to rod " + to.toText());
    solveHanoi(n-1, aux, to, f);
}