data WanderingTrader: EntityData, CreatureData, AIMobData, GrowableData {

    @Name<"DespawnDelay">
    int despawnDelay;

    @Name<"Offers">
    data {
        @Name<"Recipes">
        list<VillagerRecipe> recipes;
    } offers;

    @Name<"Inventory">
    list<Item> inventory;

    @Name<"wander_target">
    IntArray wanderTarget;
}