data Allay: EntityData, AIMobData, CreatureData {
    @Name<"CanDuplicate">
    byte canDuplicate;

    @Name<"DuplicationCooldown">
    long duplicationCooldown;

    @Name<"Inventory">
    list<Item> inventory;

    @Name<"Listener">
    Listener listener;
}

data Listener {
    data {
        float distance;
        @Name<"game_event">
        string gameEvent;
        
        list<double> pos;
        
        @Name<"projectile_owner">
        IntArray projectileOwner;
        IntArray source;
    } event;
    @Name<"event_delay">
    int eventDelay;
    @Name<"selector">
    data {
        long tick;
        data {
            float distance;
            @Name<"game_event">
            string gameEvent;
            list<double> pos;
            @Name<"projectile_owner">
            IntArray projectileOwner;
            IntArray source;
        } event;
    } _selector;
}