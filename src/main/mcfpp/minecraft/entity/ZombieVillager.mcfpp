data ZombieVillager: EntityData, CreatureData, AIMobData, ZombieData{
    @Name<"ConversionTime">
    int conversionTime;

    @Name<"ConversionPlayer">
    IntArray conversionPlayer;

    @Name<"Offers">
    data {
        @Name<"Recipes">
        list<VillagerRecipe> recipes;
    } offers;

    @Name<"Gossips">
    list<VillagerGossip> gossips;

    @Name<"VillagerData">
    data {
        int level;

        string profession;

        string type;
    } villagerData;

    @Name<"Xp">
    int xp;

}